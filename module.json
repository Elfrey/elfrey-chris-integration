{
  "id": "elfrey-chris-integration",
  "title": "Интеграция с модулем Chris's Premades",
  "description": "Быстрая и удобная настройка персонажам способностей из модуля Chris's Premades с русским переводом.",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 12,
    "verified": 12,
    "maximum": 12
  },
  "authors": [
    {
      "name": "Elfrey",
      "url": "https://github.com/Elfrey/elfrey-chris-integration",
      "discord": "elfrey"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "3.3.1",
          "verified": "3.3.1"
        }
      }
    ],
    "requires": [
      {
        "id": "chris-premades",
        "type": "module",
        "compatibility": {
          "minimum": "0.12",
          "verified": "0.12"
        }
      }
    ]
  },
  "esmodules": [
    "scripts/module.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "languages": [
    {
      "lang": "ru",
      "name": "Russian",
      "path": "language/ru.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "protected": false,
  "media": [
    {
      "type": "icon",
      "url": ""
    }
  ]
}
